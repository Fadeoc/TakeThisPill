{
  "name": "Zhihu Color Render",
  "version": "0.11B",
  "manifest_version": 3,
  "description": "color, color, how cute !~",
  "icons": {
    "16": "icons/take.jpg",
    "32": "icons/take.jpg",
    "48": "icons/take.jpg",
    "128": "icons/take.jpg"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://*.zhihu.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.zhihu.com/*"
      ],
      "js": ["scripts/zhihuColorRenderPro.js"]
    }
  ]
}
