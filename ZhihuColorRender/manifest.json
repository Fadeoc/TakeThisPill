{
  "name": "Zhihu Color Render",
  "version": "0.9.1",
  "manifest_version": 2,
  "description": "color, color, how cute !~",
  "icons": {
    "16": "icons/take.jpg",
    "32": "icons/take.jpg",
    "48": "icons/take.jpg",
    "128": "icons/take.jpg"
  },
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["*://*.zhihu.com/*"],
  "content_scripts": [
    {
      "matches": [
        "*://*.zhihu.com/*"
      ],
      "js": ["scripts/zhihuColorRenderPro.js"]
    }
  ]
}
